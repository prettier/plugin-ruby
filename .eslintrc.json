{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "prefer-destructuring": "off",
    "prefer-object-spread": "off",
    "prefer-spread": "off"
  }
}
